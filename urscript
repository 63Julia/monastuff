aobscanmodule(INJECT,MHUR.exe,C2 FF F3 0F 11 03 48 83 C4 20 5B C3 CC CC CC 40 53 48 83 EC 20) // should be unique
alloc(newmem,$1000,INJECT)

label(code)
label(return)
alloc(modifiedattack,8)


newmem:
cmp byte ptr [rcx+2E8], (int)25 // _masterCharacterId
jne @f
mov [modifiedattack], rcx

cmp byte ptr [rcx+2E9], (int)0  //_attackVariationNo[
jne @f
mov [modifiedattack], rcx

cmp [rcx+34C],(float)-1  // _attackTargetSetting._orLessHealthRate
jne @f
mov [modifiedattack],rcx

cmp [rcx+42C],(float)-1  // _grabTimeOut
jne @f
mov [modifiedattack],rcx

cmp [rcx+4A8],(float)0.25 // _MaxHoldButtonSeconds
jne @f
mov [modifiedattack],rcx

cmp byte ptr [rcx+4B1], (int)1  //_bPlayCoopVoiceAuto[1]
jne @f
mov [modifiedattack], rcx






mov [rcx+340], (int)8 //_attackTargetType AIM ALL TEAMMATES

mov [rcx+344],(float)2444442 //_attackTargetSetting._distance

mov [rcx+348], (int)0 //attackTargetCheckType

mov [rcx+4A4], (int)1 //bEnableAimingAction

mov [rcx+4A8], (int)1 //IsMagazineEmpty


code:
  push rbx
  sub rsp,20
  jmp return

  modifiedattack:
dq 00


INJECT+0F:
  jmp newmem
  nop
return:
registersymbol(INJECT modifiedattack)

